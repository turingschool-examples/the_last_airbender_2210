<h3>
  Total people who live here: <%= @member_count %>
</h3>

<% @members.each do |member| %>
  <section class="members">
    <img src=<%= member.photo_url %>>
    <p class="name">
      Name: <%= member.name %>
    </p>

    <p class="allies">
      Allies: <%= member.allies %>
    </p>

    <p class="enemies">
      Enemies: <%= member.enemies %>
    </p>

    <p class="affiliations">
      Affiliations: <%= member.affiliations %>
    </p>
  </section>
<% end %>